{
  "name": "Contentful demo",
  "description": "",
  "keywords": [
    "www"
  ],
  "scripts": {},
  "website": "https://github.com/ueno-llc/starter-kit-universally#readme",
  "repository": "https://github.com/ueno-llc/starter-kit-universally.git",
  "env": {
    "HEROKU_APP_NAME": {
      "description": "App name on Heroku",
      "required": false
    },
    "LOCAL_API_URL": {
      "description": "Local API url",
      "value": " ",
      "required": false
    },
    "NODE_ENV": {
      "description": "Environment to build",
      "required": true
    },
    "CONTENTFUL_SPACE": {
      "description": "Contentful space id",
      "required": true
    },
    "CONTENTFUL_ACCESS_TOKEN": {
      "description": "Contentful access token",
      "required": true
    },
    "CONTENTFUL_ENDPOINT": {
      "description": "Contentful enpoint",
      "required": true
    }
  },
  "environments": {
    "test": {
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "addons": [],
  "image": "heroku/nodejs",
  "buildpacks": [{
    "url": "heroku/nodejs"
  }]
}
